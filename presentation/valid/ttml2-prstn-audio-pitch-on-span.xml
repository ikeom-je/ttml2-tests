<?xml version="1.0" encoding="UTF-8"?>
<tt xml:lang="" xmlns="http://www.w3.org/ns/ttml"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    xmlns:tta="http://www.w3.org/ns/ttml#audio">
  <head>
  </head>
  <body>
    <div>
      <p begin="0s" end="3s">
        <span tta:speak="normal" tta:pitch="0">This sentence is spoken normally.</span>
      </p>        
      <p begin="3s" end="6s">
        <span tta:speak="normal" tta:pitch="-10%">This sentence should be deeper.</span>
      </p>
      <p begin="6s" end="9s">
        <span tta:speak="normal" tta:pitch="+10%">This sentence should be higher.</span>
      </p>
    </div>
  </body>
</tt>


