<?xml version="1.0" encoding="utf-8"?>
<!-- valid - region with #region-timing features - N.B. this is a subset of #timing features -->
<tt xml:lang="" xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="ttml2-transformation"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="ttml2">
  <head>
    <layout>
      <region begin="1s"/>
      <region dur="1s"/>
      <region end="1s"/>
      <region begin="1s" dur="1s"/>
      <region begin="1s" end="2s"/>
      <region dur="1s" end="2s"/>
      <region begin="1s" dur="1s" end="2s"/>
    </layout>
  </head>
</tt>
